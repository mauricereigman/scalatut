
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /assignmentA              AssignmentA.controllers.AssignmentAController.movies
GET     /assignmentB              AssignmentB.controllers.AssignmentBController.movies
GET     /assignmentC              AssignmentC.controllers.AssignmentCController.movies
GET     /assignmentD              AssignmentD.controllers.AssignmentDController.movies
GET     /done                     Done.controllers.DoneController.movies



